* {
  padding: 0;
  margin: 0;
  font-family: Lucida Sans Typewriter,Lucida Console,monaco,Bitstream Vera Sans Mono,monospace;
  font-size: 12px;
}
.navbar
  appearance: none;
  margin: 0;
  padding: 0;
  overflow: hidden;
  background-color: rgb(0,127,255);
  height: 50px;
  color: white;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16), 0 2px 10px 0 rgba(0,0,0,0.12);

.navbar-container {
  padding: 15px;
  font-size: 16px;
  font-weight: bold;
}
.center-form {
  max-width: 1024px;
  margin: auto;
  padding-top: 250px;
  text-align: center;
}
.analyze-input {
  font-size: 14px;
  text-align: center;
  border: none;
  border-bottom: 1px solid #9e9e9e;
  border-radius: 0;
  outline: none;
  height: 3rem;
  width: 100%;
  margin: 0 0 15px 0;
  padding: 0;
  box-shadow: none;
  box-sizing: content-box;
}
.analyze-button {
  position: relative;
  cursor: pointer;
  display: inline-block;
  overflow: hidden;
  background-color: rgb(0,127,255);
  width: 100%;
  height: 54px;
  line-height: 56px;
  border: none;
  color: white;
  font-size: 16px;
  text-transform: uppercase;
  outline: 0;
  font-weight: bold;
  box-shadow: 0 2px 5px 0 rgba(0,0,0,0.16), 0 2px 10px 0 rgba(0,0,0,0.12);
}

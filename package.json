{
  "name": "@media-network/analyzer",
  "version": "1.0.0",
  "description": "Crawl a URL, discover its media, then calculate how much download time and data reduced if those media is served by Media CDN.",
  "main": "src/index.js",
  "repository": "git@github.com:media-network/media-analyze.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/runtime": "7.3.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "concurrently": "3.5.1",
    "nodemon": "1.18.9",
    "run.env": "1.1.0",
    "webpack": "4.29.0"
  },
  "dependencies": {
    "boolean": "0.2.0",
    "delay": "4.1.0",
    "ect": "0.5.9",
    "express": "4.16.4",
    "mime": "2.4.0",
    "mime-match": "1.0.2",
    "mkdirp": "0.5.1",
    "mongoose": "5.4.6",
    "morgan": "1.9.1",
    "normalize-url": "2.0.1",
    "pretty-bytes": "5.1.0",
    "puppeteer": "1.11.0",
    "recursive-config": "1.0.1",
    "remove": "0.1.5",
    "shorthash": "0.0.2",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "superagent": "3.8.2"
  },
  "scripts": {
    "up": "docker-compose up -d && yarn remote",
    "remote": "docker exec -it media.crawler bash",
    "start": "yarn dev:server",
    "dev:server": "cd src && nodemon server/ --exec babel-node --CONFIG=../internals/config.dev.js",
    "build": "cd src && babel . -d ../build && cp -r server/views ../build/server/views",
    "clean": "rm -rf build",
    "docker:up": "cd internals && docker-compose up -d && cd .."
  }
}

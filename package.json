{
  "name": "@media-network/analyzer",
  "version": "1.0.0",
  "description": "Crawl a URL, discover its media, then calculate how much download time and data reduced if those media is served by Media CDN.",
  "main": "src/index.js",
  "repository": "git@github.com:media-network/media-analyzer.git",
  "author": "LongLH <d@dapps.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.3.1",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/runtime": "7.3.1",
    "babel-plugin-module-resolver": "3.1.3",
    "clean-webpack-plugin": "1.0.1",
    "concurrently": "4.1.0",
    "cors": "2.8.5",
    "glob": "7.1.3",
    "nodemon": "1.18.9",
    "normalize-url": "4.1.0",
    "run.env": "1.1.0",
    "superagent": "4.1.0",
    "wait-on": "3.2.0",
    "webpack": "4.29.0",
    "webpack-assets-manifest": "3.1.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.5.1",
    "webpack-hot-middleware": "2.24.3"
  },
  "dependencies": {
    "boolean": "0.2.0",
    "delay": "4.1.0",
    "ect": "0.5.9",
    "express": "4.16.4",
    "mime": "2.4.0",
    "mime-match": "1.0.2",
    "mkdirp": "0.5.1",
    "mongoose": "5.4.6",
    "morgan": "1.9.1",
    "pretty-bytes": "5.1.0",
    "puppeteer": "1.11.0",
    "remove": "0.1.5",
    "shorthash": "0.0.2",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0"
  },
  "scripts": {
    "start": "concurrently --raw \"npm run dev-server\" \"wait-on http://localhost:3006/alive && npm run server\"",
    "dev-server": "run.env nodemon internals/dev-server --exec babel-node --watch internals/dev-server/** --watch internals/webpack.config.babel.js",
    "server": "run.env nodemon src/server --exec babel-node --watch src/server/**",
    "clean": "rm -rf build",
    "build": "babel ./src/server -d ./build/server && cp -r ./src/server/views ./build/server",
    "webpack": "webpack --config internals/webpack.config.babel.js"
  }
}
